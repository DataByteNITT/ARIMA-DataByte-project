check_my<-function(x){
z<-data.frame()
y<-as.character(x)
a<-1
b<-1
n<-1
for(i in 1:12){
if(a<=4){
if(substring(y,i,i)==":"|substring(y,i,i)=="*"|substring(y,i,i)==""&&substring(y,i-1,i-1)!=""){
z[1,a]<-as.numeric(substring(y,b,i-1))
b<-i+1
a<-a+1
}
if(substring(y,i,i)==""&&substring(y,i-1,i-1)==""){
z[1,a]<-0
a<-a+1
}
}
}
return(z)
}
find_seq<-function(x){
z<-check_my(x[1])
for(i in 2:length(x)){
z<-rbind(z,check_my(x[i]))
}
return(z)
}
